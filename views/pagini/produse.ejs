<%- include('../fragmente/header') %>

<main id="produse-page" class="page-content">
  <%- include('../fragmente/watermark') %>

  <% for(let produs of locals.produse) { %>
    <div>Nume: <%= produs.nume %></div>
    <div>Descriere: <%= produs.descriere %></div>
    <div>Pret: <%= produs.pret %></div>
    <div>Brand: <%= produs.brand %></div>
    <div>Tip: <%= produs.tip_produs %></div>
    <div>Categorie: <%= produs.categorie %></div>
    <img height="128" src="/img/produse/<%= produs.imagine %>" alt="">
    <a href="/produse/<%= produs.id %>">Vezi mai multe detalii</a>
    <hr>
  <% } %>
</main>

<%- include('../fragmente/footer') %>
